      </article>
      <!-- #post-## -->
      <div class="toc-container" style="height: 1053px;">
      <div class="toc" style="background: none;"></div>
      </div>
      <section class="post-squares nextprev">
        <% if (post.prev){ %>
          <div class="post-nepre half previous">
            <a href="<%- url_for(post.prev.path) %>" rel="prev">
              <div class="background">
                <img class="lazyload" data-src="https://static.2heng.xin/wp-content/uploads//2018/04/IMG_20180403_23032701-1024x606.jpg" style="width: 100%; height: 100%; object-fit: cover; pointer-events: none;" onerror="imgError(this,3)" src="https://static.2heng.xin/wp-content/uploads//2018/04/IMG_20180403_23032701-1024x606.jpg">
              </div>
              <span class="label">
              Previous Post</span>
              <div class="info">
                <h3>
                <%= post.prev.title%></h3>
                <hr>
              </div>
            </a>
          </div>
        <% } %>
        <% if (post.next){ %>
          <div class="post-nepre half next">
            <a href="<%- url_for(post.next.path) %>" rel="next">
              <div class="background">
                <img class="lazyload" data-src="https://static.2heng.xin/wp-content/uploads//2018/04/发糖啦-1.png" style="width: 100%; height: 100%; object-fit: cover; pointer-events: none;" onerror="imgError(this,3)" src="https://static.2heng.xin/wp-content/uploads//2018/04/发糖啦-1.png">
